{
  "name": "legaci",
  "version": "1.0.0",
  "description": "Legacy Games Commandline installer",
  "directories": {
    "doc": "docs",
    "lib": "lib"
  },
  "engines": {
    "node": ">=10"
  },
  "bin": "./bin/legaci.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/richardregeer/legaci.git"
  },
  "author": "Richard Regeer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/richardregeer/legaci/issues"
  },
  "homepage": "https://github.com/richardregeer/legaci#readme",
  "dependencies": {
    "chalk": "^2.4.2",
    "commander": "^2.20.3",
    "shelljs": "^0.8.4",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/node": "^14.6.2",
    "@types/shelljs": "^0.8.8",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "ava": "^3.13.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.8",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "mock-fs": "^4.10.1",
    "nyc": "^15.0.0",
    "prettier": "^2.1.2",
    "sinon": "^7.4.1",
    "typescript": "^4.0.2"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "tests/**",
      "src/main.js"
    ],
    "check-coverage": true,
    "branches": 85,
    "lines": 85,
    "functions": 85,
    "statements": 85,
    "watermarks": {
      "lines": [
        70,
        85
      ],
      "functions": [
        70,
        85
      ],
      "branches": [
        70,
        85
      ],
      "statements": [
        70,
        85
      ]
    }
  }
}
